{
  "name": "syntax-tree-viewer",
  "displayName": "Syntax Tree Viewer",
  "description": "Visualizes Syntax Trees",
  "version": "0.0.1",
  "publisher": "hediet",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:syntaxtree.show"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "syntaxtree.show",
        "title": "Show Syntax Tree"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./ && cd resources && npm run bundle",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "typescript": "^2.0.9"
  }
}
